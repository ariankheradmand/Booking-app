generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id        Int      @id @default(autoincrement())
  name      String
  weeks     String
  hours     String   
  service   String
  phoneNumber String
  createdAt DateTime @default(now())
}
